{
  "name": "sztyup/acl",
  "description": "Light-weight role-based permissions for Laravel 5 with a great degree of customization",
  "license": "LGPL-3.0",
  "keywords": [
    "laravel",
    "permissions",
    "roles",
    "auth",
    "acl",
    "security"
  ],
  "authors": [
    {
      "name": "Sztyup",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "~6.0|~7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~5.3",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Sztyup\\Acl\\": "src/Acl"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Acl\\AclServiceProvider"
      ]
    }
  }
}
